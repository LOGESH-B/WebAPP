

<%- layout("boiler")-%>

<%- include("nav")-%>
<%- include("clubnav")-%>



<%console.log('hii'+ found) %> 
<% console.log(found.author) %> 
<% console.log(currentUser._id) %> 
 
 
 <div class="container d-flex ">
 <% for(let event of found.events){ %>
 
  <div class="card " style="width: 18rem;">
   
    
    <div class="card-body">
      <form action="/club/<%=found.clubname%>/<%= event._id%>" method="get">
     
        <% console.log("boii",event._id) %> 
      <h5 class="card-title"><%= event.eventname  %> </h5>
      <p class="card-text"><%= event.eventdiscription  %></p>
      <p class="text-muted"><%= event.eventdate %></p>
      <p class="text-muted"><%= event.enddate %></p>
      <button type="submit" class="btn btn-info">View Event</button>

    </form>
    
      <div class="d-flex ">
        
        <% if (found.author.equals(currentUser._id)) { %>

          <form action="/club/<%=found.clubname%>/<%=event._id %>/edit" method="get">
            <button class="btn btn-sm btn-primary" >Edit</button>
          </form>
          
          <form  action="/club/<%=found.clubname%>/<%=event._id %>/delete?_method=DELETE" method="post">
            <button class="btn btn-sm btn-danger" >Delete</button>
          </form>
         
        <% } %>
        
    
    </div>
    </div>
  </div>

  <% } %>  
  <% if (found.author.equals(currentUser._id)) { %>
  <div class="container justify-content-center"> 
    <form action="/club/<%=found.clubname%>/add" method="get">
    <button class="btn btn-success btn-md" >ADD Event</button>
  </form>
  </div>
  <% } %>

</div>
