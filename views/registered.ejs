<%- layout("boiler")-%>
<%- include("nav")-%>

<script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.17.0/xlsx.full.min.js"></script>
<script src="/js/pdf.js"></script>

<div class="container mt-3" id="invoice">
  <h1 class="text-center mt-3"><%=foundregister.eventname %></h1>
<table class="table table-striped table-hover " id="register_table">
    <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Name</th>
          <th scope="col">Roll No</th>
          <th scope="col">Dept</th>
          <th scope="col">email</th>
          <th scope="col">Team Member</th>
        </tr>
      </thead>
      <tbody>
        <% foundregister.register.forEach((register,i) => {%>
          <tr>
            <th scope="row"><%=(i+1)%></th>
            <td><%=register.name %> </td>
            <td><%=register.rollno %> </td>
            <td><%=register.department %> </td>
            <td><%=register.username %> </td>
            <td><%=register.member %> </td>
          </tr>
          
        <%} )%> 
        


        
        
      
      </tbody>
  </table>
  
</div>
<div class="container d-flex justify-content-around mt-5">
  <button class="btn btn-danger " id="download">Download as pdf</button>
  <button class="btn btn-success" onclick="export_data()" id="downloadexcel">Download as excel</button>

</div>

<footer class="footer1" >
  
  <div class="py-3 ml-4  d-flex justify-content-evenly flex-wrap " >
  
